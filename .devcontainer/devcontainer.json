{
  "name": "terakoya-client-static-node18",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "node18",
  "workspaceFolder": "/app",
  "customizations": {
    "vscode": {
      "settings": {
        "editor.formatOnSave": true,
        "files.exclude": {
          "tmp": true
        }
      },
      "extensions": [
        "esbenp.prettier-vscode",
        "dbaeumer.vscode-eslint",
        "orta.vscode-jest",
        "github.copilot"
      ]
    }
  }
}
